<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Chatting">
	
	<insert id="createChatting">
		insert into chatting values(chatting_seq.nextval, #{project_seq}, #{title}, default, #{member_seq}, #{member_name}, sysdate, default)
	</insert>
	
	
	<insert id="insertMainMember">
		insert into chatting values(chatting_seq.nextval, #{project_seq}, #{title}, #{member_count}, #{member_seq}, #{member_name}, #{create_date}, default)
	</insert>
	
	
</mapper>